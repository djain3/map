          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "background-color": "@bg",
            "& when (@bgFigures)": {
              "& when (@figure = '1')": {
                ".figure": {
                  "background-color": "@circleColor",
                  "width": "(@circleRadius * 1px)",
                  "height": "(@circleRadius * 1px)",
                  "border-radius": "50%",
                  "position": "absolute",
                  "top": "(@circlePosY * 1%)",
                  "left": "(@circlePosX * 1%)",
                  "z-index": "11"
                },
                ".container": {
                  "z-index": "22"
                },
                "& when (@overflow)": {
                  "overflow": "hidden"
                },
                "& when not (@overflow)": {
                  "overflow": "visible"
                }
              },
              "& when (@figure = '2')": {
                ".figure": {
                  "background-color": "@rectColor",
                  "position": "absolute",
                  "left": "0",
                  "top": "0",
                  "width": "(@sizeRect * 1%)",
                  "height": "100%",
                  "& when (@invertRect)": {
                    "left": "auto",
                    "right": "0"
                  },
                  "& when (@directRect)": {
                    "width": "100%",
                    "height": "(@sizeRect * 1%)",
                    "& when (@invertRect)": {
                      "top": "auto",
                      "bottom": "0"
                    }
                  }
                }
              },
              "& when (@figure = '3')": {
                ".figure": {
                  "background": "linear-gradient(@rotateAngle * 1deg, @diagonalColor (@sizeDiagonal * 1%), rgba(0, 0, 0, 0) (@sizeDiagonal * 1%), rgba(0, 0, 0, 0) 100%)",
                  "position": "absolute",
                  "left": "0",
                  "top": "0",
                  "width": "100%",
                  "height": "100%"
                }
              }
            },
            "& when (@reverseContent)": {
              "> .mbr-row": {
                "flex-direction": "row-reverse"
              }
            },
            ".mbr-row": {
              "height": "100%",
              "z-index": "22",
              "justify-content": "center"
            },
            ".map-placeholder": {
              "display": "none",
              "h4": {
                "padding-top": "5rem",
                "color": "#767676",
                "text-align": "center"
              }
            },
            "@media (max-width: 992px)": {
              ".map-container": {
                "margin-top": "2rem"
              }
            },
            ".google-map": {
              "min-height": "(@mapHeight * 1px)",
