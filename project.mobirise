          "_customHTML": "<section class=\"footer2 map\" group=\"footer\" plugins=\"amp-main,amp-iframe\">\n\t<mbr-parameters>\n\t<!-- Block parameters controls (Blue \"Gear\" panel) -->\n\t<!-- Section Padding -->\n\t<input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"2\">\n\t<input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"2\">\n\t<!-- Map Settings  -->\n\t<input type=\"map\" title=\"Map\" name=\"googleMap\">\n\t<input type=\"range\" inline=\"\" title=\"Map Height\" name=\"mapHeight\" min=\"300\" max=\"900\" step=\"50\" value=\"700\">\n\t<input type=\"checkbox\" title=\"Map on Left/Right\" name=\"reverseContent\" checked=\"\">\n\t<!-- Show/Hide Elements -->\n\t<input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked=\"\">\n\t<input type=\"checkbox\" title=\"Show Text\" name=\"showText\" checked=\"\">\n\t<!-- Background Settings -->\n\t<input type=\"color\" title=\"Background Color\" name=\"bg\" value=\"#ffffff\">\n\t<input type=\"checkbox\" title=\"Show BG Figures\" name=\"bgFigures\" checked=\"\">\n\t<!-- Select Figure -->\n\t<select title=\"Select Figure:\" name=\"figure\" condition=\"bgFigures\">\n\t\t<option value=\"1\">Circle</option>\n\t\t<option value=\"2\" selected=\"\">Rectangle</option>\t\n\t\t<option value=\"3\">Diagonal</option>\n\t</select>\n\t<!-- Figure Circle -->\n\t<input type=\"range\" inline=\"\" title=\"Circle Position X\" name=\"circlePosX\" min=\"-100\" max=\"100\" step=\"5\" value=\"40\" condition=\"figure==1\">\n\t<input type=\"range\" inline=\"\" title=\"Circle Position Y\" name=\"circlePosY\" min=\"-100\" max=\"100\" step=\"5\" value=\"-40\" condition=\"figure==1\">\n\t<input type=\"range\" inline=\"\" title=\"Circle Radius\" name=\"circleRadius\" min=\"100\" max=\"1600\" step=\"50\" value=\"1200\" condition=\"figure==1\">\n\t<input type=\"color\" title=\"Circle Color\" name=\"circleColor\" value=\"#9dc7d3\" condition=\"figure==1\">\n\t<input type=\"checkbox\" title=\"Overflow Hidden\" name=\"overflow\" condition=\"figure==1\" checked=\"\">\n\t<!-- Figure Rectangle -->\n\t<input type=\"checkbox\" title=\"Width/Height\" name=\"directRect\" condition=\"figure==2\" checked=\"\">\n\t<input type=\"checkbox\" title=\"Invert\" name=\"invertRect\" condition=\"figure==2\" checked=\"\">\n\t<input type=\"range\" inline=\"\" title=\"Size\" name=\"sizeRect\" min=\"0\" max=\"100\" step=\"5\" value=\"40\" condition=\"figure==2\">\n\t<input type=\"color\" title=\"Rectangle Color\" name=\"rectColor\" value=\"#9dc7d3\" condition=\"figure==2\">\n\t<!-- Figure Diagonal -->\n\t<input type=\"range\" inline=\"\" title=\"Rotate Angle\" name=\"rotateAngle\" min=\"0\" max=\"360\" step=\"5\" value=\"45\" condition=\"figure==3\">\n\t<input type=\"checkbox\" title=\"Invert\" name=\"invertDiagonal\" condition=\"figure==3\" checked=\"\">\n\t<input type=\"range\" inline=\"\" title=\"Size\" name=\"sizeDiagonal\" min=\"0\" max=\"100\" step=\"5\" value=\"50\" condition=\"figure==3\">\n\t<input type=\"color\" title=\"Diagonal Color\" name=\"diagonalColor\" value=\"#9dc7d3\" condition=\"figure==3\">\n\t<!-- End block parameters -->\n\t</mbr-parameters>\n\t<div class=\"figure\"></div>\n\t<div class=\"mbr-row\">\n\t\t<div class=\"mbr-col-lg-8 mbr-col-md-12 mbr-col-sm-12 map-container\">\n\t\t\t<div class=\"google-map\" mbr-map=\"googleMap\"></div>\n\t\t\t<div class=\"map-placeholder\">\n\t\t\t\t<div placeholder=\"\">\n\t\t\t\t\t<div class=\"mobirise-spinner\">\n\t\t\t\t\t\t<em></em>\n\t\t\t\t\t\t<em></em>\n\t\t\t\t\t\t<em></em>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t</div>\n\t\t<div class=\"mbr-col-lg-4 mbr-col-md-12 mbr-col-sm-12 right-container\">\n\t\t\t<div class=\"text-block\">\n\t\t\t\t<h3 class=\"mbr-section-title mbr-black mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\">\n\t\t\t\tUnited States\n\t\t\t\t</h3>\n\t\t